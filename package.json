{
  "name": "grainhero",
  "version": "1.0.0",
  "description": "GrainHero - AI-Powered Grain Storage Management System",
  "scripts": {
    "dev": "node start-dev.js",
    "dev:backend": "cd farmHomeBackend-main && npm run dev",
    "dev:frontend": "cd farmHomeFrontend-main && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd farmHomeBackend-main && npm start",
    "start:frontend": "cd farmHomeFrontend-main && npm start",
    "install:all": "npm install && cd farmHomeBackend-main && npm install && cd ../farmHomeFrontend-main && npm install",
    "build": "cd farmHomeFrontend-main && npm run build",
    "build:backend": "cd farmHomeBackend-main && npm install",
    "build:frontend": "cd farmHomeFrontend-main && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "grain",
    "storage",
    "ai",
    "agriculture",
    "management"
  ],
  "author": "GrainHero Team",
  "license": "MIT",
  "dependencies": {
    "stripe": "^19.1.0"
  }
}
